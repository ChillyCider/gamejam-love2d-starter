{
    "workspace.library": [
        "${addons}/love2d/module/library"
    ],
    "runtime.version": "Lua 5.3",
    "runtime.builtin": {
        "utf8": "enable"
    },
    "runtime.special": {
        "love.filesystem.load": "loadfile"
    },
    "runtime.path": [
        "src/?.lua",
        "src/?/init.lua"
    ],
    "hint.enable": false,
    "completion.keywordSnippet": "Disable",
    "completion.showWord": "Disable",
    "completion.callSnippet": "Disable",
    "diagnostics.disable": [
        "empty-block",
        "lowercase-global"
    ]
}
